
<div class="message">
    <% if(success && success.length){ %>
        <div class="alert-box">
            <i class="fa-solid fa-circle-check" style="color: green;"></i><%= success %>
        </div>
    <% } %>
    <% if(error && error.length){ %>
        <div class="alert-box alert-box-err">
            <i class="fa-solid fa-circle-xmark" style="color: red;"></i><%= error %>
        </div>
    <% } %>
</div>